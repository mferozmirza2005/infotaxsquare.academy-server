{
  "compilerOptions": {
    "target": "ES6",
    "outDir": "build",
    "strict": true,
    "module": "ES6",
    "rootDir": "src",
    "paths": {
      "@/*": ["./*"]
    },
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "moduleResolution": "Bundler"
  }
}